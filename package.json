{
  "name": "st-mirage-controller",
  "version": "0.0.1",
  "scripts": {
    "build": "vite build",
    "watch": "vite build --watch --mode development",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,scss,html,vue}\"",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "wasm:build:dev": "cd wasm && wasm-pack build --target web --dev",
    "wasm:build": "cd wasm && wasm-pack build --target web --release",
    "wasm:watch": "cd wasm && cargo watch -i wasm -s 'wasm-pack build --target web --dev'",
    "wasm:optimize": "cd wasm && wasm-opt -O1 --enable-bulk-memory --enable-mutable-globals --enable-nontrapping-float-to-int --enable-sign-ext --enable-simd --enable-reference-types pkg/wasm_bg.wasm -o pkg/wasm_bg.wasm",
    "wasm:inspect": "cd wasm && wasm2wat ./*.wasm > ./output.wat && wc -l ./output.wat"
  },
  "browserslist": [
    "defaults and partially supports es6-module"
  ],
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@playwright/test": "^1.56.1",
    "@popperjs/core": "^2.11.8",
    "@sqliteai/sqlite-wasm": "3.50.4-sync.0.8.52-vector.0.9.46",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/jquery": "^3.5.33",
    "@types/jqueryui": "^1.12.24",
    "@types/lodash": "^4.17.20",
    "@types/mark.js": "^8.11.12",
    "@types/toastr": "^2.1.43",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "css-loader": "^7.1.2",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-better-tailwindcss": "^3.7.10",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-pinia": "^0.4.1",
    "eslint-plugin-vue": "^10.5.1",
    "globals": "^16.4.0",
    "happy-dom": "^20.0.10",
    "highlight.js": "^11.11.1",
    "postcss-load-config": "^6.0.1",
    "postcss-loader": "^8.2.0",
    "postcss-minify": "^1.2.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "6.0.0-dev.20250807",
    "typescript-eslint": "^8.46.2",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-vue-components": "^29.2.0",
    "vite": "^7.1.12",
    "vite-plugin-external": "^6.2.2",
    "vite-plugin-wasm-pack": "^0.1.12",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-loader": "^17.4.2",
    "vue-router": "^4.6.3",
    "vue-style-loader": "^4.1.3",
    "yaml": "^2.8.1"
  },
  "dependencies": {
    "@vueuse/components": "^13.9.0",
    "@vueuse/core": "^13.9.0",
    "@vueuse/integrations": "^13.9.0",
    "@vueuse/shared": "^13.9.0",
    "klona": "^2.0.6",
    "pinia": "^3.0.3",
    "tailwindcss": "^4.1.16",
    "type-fest": "^4.41.0",
    "vue": "^3.5.22",
    "vue-final-modal": "^4.5.5",
    "zod": "^4.1.12"
  }
}
